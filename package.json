{
  "name": "@caeljs/tsh",
  "type": "module",
  "description": "A flexible typed data validation library that allows you to define schemas with TypeScript and validate data at runtime.",
  "version": "1.1.0",
  "main": "cjs/Tsh.cjs",
  "module": "esm/Tsh.js",
  "types": "Tsh.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drylian/tsh.git"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./Tsh.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./Tsh.d.ts",
      "import": "./esm/Tsh.js",
      "require": "./cjs/Tsh.cjs"
    }
  },
  "keywords": [
    "backend",
    "standardization",
    "standard",
    "env",
    "config-js",
    "json",
    "config",
    "tsh",
    "typed",
    "npm",
    "node",
    "bun",
    "deno",
    "typescript",
    "debug",
    "output",
    "performance"
  ],
  "author": "Drylian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drylian/tsh/issues"
  },
  "homepage": "https://github.com/drylian/tsh#readme",
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "@types/node": "^22.13.10",
    "dts-bundle-generator": "^9.5.1",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  }
}
